<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="TitleBar">
        <attr name="tbTitle" format="string"/>
        <attr name="tbOptions" format="reference"/>
    </declare-styleable>
    
    <declare-styleable name="MenuChildItem">
        <attr name="mciIcon" format="reference"/>
        <attr name="mciDescription" format="string"/>
    </declare-styleable>

    <declare-styleable name="TaskBarItem">
        <attr name="tbiIcon" format="reference"/>
        <attr name="tbiDescription" format="string"/>
    </declare-styleable>

    <declare-styleable name="UnderlinedEditText">
        <attr name="uetColor" format="color"/>
    </declare-styleable>

    <declare-styleable name="TouchInterceptorLayout">
        <attr name="tilIdentifier" format="string"/>
    </declare-styleable>

    <declare-styleable name="MaxHeightScrollView">
        <attr name="maxHeight" format="dimension"/>
    </declare-styleable>

    <declare-styleable name="MaxDimensionLayout">
        <attr name="maxDimensionHeight" format="dimension"/>
        <attr name="maxDimensionWidth" format="dimension"/>
    </declare-styleable>
</resources>